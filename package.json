{
  "name": "group-01-match-making-system-cosc2408-",
  "version": "1.0.0",
  "description": " Group-01-Match-Making-System-COSC2408-",
  "main": "firebase emulators:start",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "firebase emulators:start",
    "deploy": "firebase deploy",
    "test": "run-script-os",
    "test:windows": "firebase emulators:exec \"jest --forceExit --detectOpenHandles\" | findstr /v /R \"\\<hosting \\<firestore \\<auth \\<hub \\<emulators \\<functions\"",
    "test:nix": "firebase emulators:exec \"jest --forceExit --detectOpenHandles\" | grep -v \"hosting*\\|firestore*\\|auth\\|hub\\|emulators\\|functions\"",
    "posttest": "run-script-os",
    "posttest:windows": "kill-port -s 9099 5001 8080 5000 4400 > nul 2>&1",
    "posttest:nix": "kill-port -s 9099 5001 8080 5000 4400 > /dev/null 2>&1",
    "login": "firebase login"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Group-01-Match-Making-System-COSC2408/Group-01-Match-Making-System-COSC2408-.git"
  },
